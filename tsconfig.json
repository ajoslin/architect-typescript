{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": "src",
    "paths": {
      "@architect/shared/*": ["./source/shared/*"]
    }
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "source/**/*.ts"
  ]
}
